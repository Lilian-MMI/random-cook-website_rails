<h1><%= params[:user_name]%></h1>

<%= link_to "Nouvelle Recette", "/recipe?user_name="+params[:user_name] %>

<ul>
  <% @favorites.each do |fav| %>
    <li>
        <%= fav.id %>
    </li>
  <% end %>
</ul>

<ul>
  <% @recipes.each do |recipe| %>
    <li>
    <%= link_to recipe.label, "/recipe/"+recipe.id.to_s %>
    </li>
  <% end %>
</ul>
